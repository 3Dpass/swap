{
  "name": "dot-acp-ui",
  "private": true,
  "version": "0.0.1",
  "author": "MVP Workshop",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "knip": "knip",
    "preview": "vite preview",
    "commit": "cz",
    "test": "jest",
    "check:packages": "pnpm up --latest --interactive",
    "prepare": "is-ci || husky",
    "postinstall": "is-ci || husky || true",
    "setup": "pnpm install && husky"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm prettier --write",
      "pnpm eslint --fix",
      "bash -c 'tsc --noEmit'"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@metamask/jazzicon": "^2.0.0",
    "@polkadot/api": "^10.13.1",
    "@polkadot/api-augment": "^10.13.1",
    "@polkadot/extension-inject": "0.46.9",
    "@polkadot/react-identicon": "^3.16.1",
    "@polkadot/types": "^10.13.1",
    "@polkadot/util": "^12.6.2",
    "@polkadot/util-crypto": "^12.6.2",
    "@sentry/react": "^7.110.1",
    "@talismn/connect-wallets": "^1.2.5",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "classnames": "^2.5.1",
    "decimal.js": "^10.4.3",
    "ethers": "5.7.2",
    "i18next": "^23.11.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jazzicon": "^1.5.0",
    "lottie-react": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.1.0",
    "react-number-format": "^5.3.4",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.1.7",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "knip": "^5.58.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sass": "^1.64.2",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "vite": "^5.2.9",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-mkcert": "^1.16.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "overrides": {
    "braces@<3.0.3": ">=3.0.3",
    "ws@>=8.0.0 <8.17.1": ">=8.17.1",
    "axios@>=1.3.2 <=1.7.3": ">=1.7.4",
    "micromatch@<4.0.8": ">=4.0.8",
    "vite@>=5.2.0 <5.2.14": ">=5.2.14",
    "rollup@>=4.0.0 <4.22.4": ">=4.22.4",
    "@sentry/browser@<7.119.1": ">=7.119.1",
    "nanoid@<3.3.8": ">=3.3.8",
    "@octokit/request-error@>=1.0.0 <5.1.1": ">=5.1.1",
    "@octokit/endpoint@>=9.0.5 <9.0.6": ">=9.0.6",
    "esbuild@<=0.24.2": ">=0.25.0",
    "vite@>=5.0.0 <=5.4.11": ">=5.4.12",
    "@octokit/request@>=1.0.0 <8.4.1": ">=8.4.1",
    "@octokit/plugin-paginate-rest@>=1.0.0 <9.2.2": ">=9.2.2",
    "vite@>=5.0.0 <5.4.15": ">=5.4.15",
    "axios@>=1.0.0 <1.8.2": ">=1.8.2",
    "vite@>=5.0.0 <5.4.16": ">=5.4.16",
    "vite@>=5.0.0 <5.4.18": ">=5.4.18",
    "@babel/runtime@<7.26.10": ">=7.26.10",
    "@babel/helpers@<7.26.10": ">=7.26.10",
    "vite@>=5.0.0 <=5.4.18": ">=5.4.19",
    "vite@>=5.0.0 <5.4.17": ">=5.4.17",
    "cross-spawn@>=7.0.0 <7.0.5": ">=7.0.5"
  }
}
